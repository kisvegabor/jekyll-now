        <type>lv_opa_t</type>
        <definition>lv_opa_t lv_draw_aa_get_opa</definition>
        <argsstring>(lv_coord_t seg, lv_coord_t px_id, lv_opa_t base_opa)</argsstring>
        <name>lv_draw_aa_get_opa</name>
        <param>
          <type>lv_coord_t</type>
